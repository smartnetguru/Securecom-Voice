    <string name="CallMetricsInfo__metrics_collection_details">To help improve call quality, you can choose to send Open WhisperSystems information that will help us to better understand Securecom Voice\'s performance.  This data is never visible to a third party.  All call metrics data is encrypted before it is stored or uploaded to Open WhisperSystem\'s servers.  When enabled, the following information is collected during each call:</string>
    <string name="CallMetricsInfo__title">Quality Metrics</string>
    <string name="CallMetricsInfo__device_description">Device Description</string>
    <string name="CallMetricsInfo__device_description_explanation">Model, manufacturer, carrier, connection type, OS version, persistent random UUID</string>
    <string name="CallMetricsInfo__mic_and_codec_performance">Microphone and Codec Performance</string>
    <string name="CallMetricsInfo__mic_and_codec_performance_explanation">Mic clipping, dropped audio samples, average call volume</string>
    <string name="CallMetricsInfo__packet_arrival">Packet arrival interval histogram</string>
    <string name="CallMetricsInfo__packet_arrival_explanation">Provides data on network latency jitter</string>
    <string name="CallMetricsInfo__out_of_order_arrival">Out of order arrival counts</string>
    <string name="CallMetricsInfo__out_of_order_arrival_explanation">Tracks packet delivery irregularities</string>
    <string name="CallMetricsInfo__audio_gap_fill_counts">Audio Gap Fills</string>
    <string name="CallMetricsInfo__audio_gap_fill_counts_explanation">Indicates when missing data required audio to be synthesized</string>
    <string name="CallMetricsInfo__system_metrics">System Metrics</string>
    <string name="CallMetricsInfo__system_metrics_explanation">CPU load, memory usage</string>

